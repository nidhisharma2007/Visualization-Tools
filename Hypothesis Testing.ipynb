   "source": [
    "#Assignment 4\n",
    "\n",
    "mean = 85\n",
    "sample_mean = 88\n",
    "sigma = 1\n",
    "z_critical = 1.96\n",
    "\n",
    "z = (sample_mean - mean) / sigma\n",
    "print(\"Z-score:\", z)\n",
    "\n",
    "if z > z_critical:\n",
    "    print(\"Reject Null Hypothesis\")\n",
    "else:\n",
    "    print(\"Accept Null Hypothesis\")\n"
   ]
